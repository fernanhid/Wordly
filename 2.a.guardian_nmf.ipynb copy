    "## Data Cleaning"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "final_data = pd.DataFrame(transformed_data).applymap(lambda x: 1 if x>0.2 else 0)\n",
    "final_data.columns = topics.values()\n",
    "final_data['month'] = [i for i in guardian.month]\n",
    "\n",
    "by_month = final_data.groupby('month').sum()\n",
    "for i in range(8):\n",
    "    by_month.iloc[i] = by_month.iloc[i]/float(np.sum(by_month.iloc[i]))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 380,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "by_month.to_pickle('guardian_month.pkl')"
   ]
