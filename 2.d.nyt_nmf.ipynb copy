   "source": [
    "## Cleaning Up Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 284,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "final_data = pd.DataFrame(transformed_data).applymap(lambda x: 1 if x>.1 else 0)\n",
    "final_data.columns = topics.values()\n",
    "final_data['month'] = [i for i in nyt.month]\n",
    "\n",
    "by_month = final_data.groupby('month').sum()\n",
    "for i in range(8):\n",
    "    by_month.iloc[i] = by_month.iloc[i]/float(np.sum(by_month.iloc[i]))\n",
    "\n",
    "by_month['Brazil Corruption'] = by_month['Brazil Corruption 1'] + by_month['Brazil Corruption 2'] + by_month['Brazil Corruption 3']\n",
    "del by_month['Brazil Corruption 1']\n",
    "del by_month[\"Brazil Corruption 2\"]\n",
    "del by_month[\"Brazil Corruption 3\"]"
   ]
  },
