   },
   "outputs": [],
   "source": [
    "# Cleaning Up Data\n",
    "final_data = pd.DataFrame(transformed_data).applymap(lambda x: 1 if x>.15 else 0)\n",
    "final_data.columns = topics.values()\n",
    "final_data['month'] = [i for i in age.month]\n",
    "\n",
    "by_month = final_data.groupby('month').sum()\n",
    "for i in range(7):\n",
    "    by_month.iloc[i] = by_month.iloc[i]/float(np.sum(by_month.iloc[i]))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 267,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
