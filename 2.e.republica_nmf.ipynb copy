   "source": [
    "## Cleaning Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 444,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "# Final Data is By_Month\n",
    "final_data = pd.DataFrame(transformed_data).applymap(lambda x: 1 if x>.12 else 0)\n",
    "final_data.columns = topics.values()\n",
    "final_data['month'] = republica.month\n",
    "by_month = final_data.groupby('month').sum()\n",
    "\n",
    "for i in range(8):\n",
    "    by_month.iloc[i] = by_month.iloc[i]/float(np.sum(by_month.iloc[i]))\n",
    "    \n",
    "by_month['Brazil Corruption'] = by_month['Brazil Corruption 1'] + by_month['Brazil Corruption 2']\n",
    "del by_month['Brazil Corruption 1']\n",
    "del by_month[\"Brazil Corruption 2\"]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "by_month.to_pickle('republica_month.pkl')"
   ]
  },
